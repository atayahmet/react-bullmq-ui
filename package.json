{
  "name": "react-bullmq-ui",
  "version": "0.1.4",
  "description": "A React UI component for listing, managing, and visualizing BullMQ jobs, built with Ant Design.",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "NODE_ENV=test jest",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "keywords": [
    "react",
    "bullmq",
    "ui",
    "typescript",
    "jobs",
    "queue",
    "dashboard",
    "ant-design",
    "antd"
  ],
  "author": "Ahmet Atay",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atayahmet/react-bullmq-ui.git"
  },
  "bugs": {
    "url": "https://github.com/atayahmet/react-bullmq-ui/issues"
  },
  "homepage": "https://github.com/atayahmet/react-bullmq-ui#readme",
  "peerDependencies": {
    "@ant-design/icons": "^6.0.0",
    "antd": "^5.25.1",
    "bullmq": "^5.52.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.11",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.3"
  }
}
